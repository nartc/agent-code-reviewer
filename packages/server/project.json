{
  "name": "server",
  "projectType": "application",
  "sourceRoot": "packages/server/src",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsx watch packages/server/src/index.ts",
        "cwd": "{workspaceRoot}"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run",
        "cwd": "packages/server"
      }
    }
  }
}
